{
  "name": "cv-rank-generator",
  "version": "1.0.0",
  "description": "A web application for CV ranking and generation using AI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "dotenv": "^16.3.1",
    "docx": "^8.5.0",
    "puppeteer": "^21.5.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["cv", "ai", "ranking", "generation"],
  "author": "",
  "license": "MIT"
}
